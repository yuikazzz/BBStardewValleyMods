{
    "LogName": "Basic Tea Rules",
    "Action": "EditData",
    "Target": "Data/Machines",
    "When":  {"Atelier.Cauldron/ConfigValue:Tea": true},
    "Entries": {
        "(BC)Wildflour.AtelierGoods_TeaProcessor": {
            "OutputRules": [
                {
                    "Id": "Wildflour.AtelierGoods_Flavored_Sachet",
                    "Triggers": [
                        {
                        "Trigger": "ItemPlacedInMachine",
                        "RequiredTags": ["category_fruits"],
                        "RequiredCount": 1
                        },
                        {
                        "Trigger": "ItemPlacedInMachine",
                        "RequiredTags": ["edible_flower_item"],
                        "RequiredCount": 1
                        }
                ],
                    "OutputItem": [
                        {
                        "Id": "Wildflour.AtelierGoods_Flavored_Sachet",
                        "ItemId": "Wildflour.AtelierGoods_Flavored_Sachet",
                        "ObjectDisplayName": "[LocalizedText Strings\\Objects:Wildflour.AtelierGoods_Flavored_Sachet %PRESERVED_DISPLAY_NAME]",
                        "ObjectInternalName": "{0} Tea Sachet",  
                        "PreserveID":"DROP_IN",
                        "CopyColor":true,
                        "CopyQuality": true,
                        "CopyPrice": true,
                        "PriceModifiers": 
                            [ 
                                {
                                    "Modification": "Multiply",
                                    "Amount": 1.4
                                },
                                {
                                    "Modification": "Add",
                                    "Amount": 50
                                }
                            ]
                        }
                    ],
                    "MinutesUntilReady": 1000
                }               
            ],
            "AdditionalConsumedItems":[
                {
                    "ItemId":"(O)815",
                    "RequiredCount":1,
                    "InvalidCountMessage":"{{i18n:AtelierGoods_TeaProcessor_InvalidCount.message}}"
                }
            ],
            "InvalidItemMessage":"{{i18n:AtelierGoods_TeaProcessor_InvalidItem.message}}",
            "ShowNextIndexWhileWorking":false,
            "ShowNextIndexWhenReady":false,
            "WobbleWhileWorking":false,
            "ReadyTimeModifiers": [
                {
                    "Id": "TeaTimeSpeedy",
                    "Condition": "LOCATION_CONTEXT Target HXW.Buildings_TeaShop_TeaShopInterior",
                    "Modification": "Multiply",
                    "Amount": 0.75
                },
                {
                    "Id": "TeaTimeSuperSpeedy",
                    "Condition": "LOCATION_CONTEXT Target HXW.Buildings_TeaShop_TeaShopInteriorLarge",
                    "Modification": "Multiply",
                    "Amount": 0.5
                }
            ],
            "WorkingEffects":[
                {
                    "Frames":[1,2,3,4,5,6],
                    "Interval":100
                }
           ] 
        },
        "(BC)Wildflour.AtelierGoods_TeaKettle": {
            "OutputRules": [
                {
                    "Id": "Wildflour.AtelierGoods_Flavored_Tea",
                    "Triggers": [
                        {
                        "Trigger": "ItemPlacedInMachine",
                        "RequiredItemId": "(O)Wildflour.AtelierGoods_Flavored_Sachet",
                        "RequiredCount": 1
                        }
                ],
                    "OutputItem": [
                        {
                        "Id": "Wildflour.AtelierGoods_Flavored_Tea",
                        "ItemId": "(O)Wildflour.AtelierGoods_Flavored_Tea",
                        "ObjectDisplayName": "[LocalizedText Strings\\Objects:Wildflour.AtelierGoods_Flavored_Tea %PRESERVED_DISPLAY_NAME]",
                        "ObjectInternalName": "{0} Tea",
                        "PreserveID":"DROP_IN_PRESERVE",
                        "CopyColor":true,
                        "CopyQuality": true,
                        "CopyPrice": true,
                        "PriceModifiers": [
                            {
                                "Modification": "Multiply",
                                "Amount": 4.0
                            },
                            {
                                "Modification": "Add",
                                "Amount": 100
                            }
                        ]
                    }
                ],
                "MinutesUntilReady": 300
                }
            ],
            "AdditionalConsumedItems":[
                {
                    "ItemId":"(O)340",
                    "RequiredCount":1,
                    "InvalidCountMessage":"{{i18n:AtelierGoods_TeaKettle_InvalidCount.message}}"
                }
            ],
            "InvalidItemMessage":"{{i18n:AtelierGoods_TeaKettle_InvalidItem.message}}",
            "ShowNextIndexWhileWorking":false,
            "ShowNextIndexWhenReady":false,
            "WobbleWhileWorking":false,
            "ReadyTimeModifiers": [
                {
                    "Id": "TeaTimeSpeedy",
                    "Condition": "LOCATION_CONTEXT Target HXW.Buildings_TeaShop_TeaShopInterior",
                    "Modification": "Multiply",
                    "Amount": 0.75
                },
                {
                    "Id": "TeaTimeSuperSpeedy",
                    "Condition": "LOCATION_CONTEXT Target HXW.Buildings_TeaShop_TeaShopInteriorLarge",
                    "Modification": "Multiply",
                    "Amount": 0.5
                }
            ],
            "WorkingEffects":[
                {
                    "Frames":[1,2,3,4,5],
                    "Interval":100
                }
           ] 
        }
    }
},


