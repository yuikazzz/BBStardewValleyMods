  "DynamicTokens": [
    //Tokens for Gift Dialogue Compat
    {
      "Name": "ModdedDialogCompat.GiftTaste",
      "Value": "true" //generally my content should be applied
    },
    {
      "Name": "ModdedDialogCompat.GiftTaste",
      "Value": "{{Random: true, false}}", //randomize applying my content when these mods are present
      "When": {
        "HasMod |contains= PrincessFelicie.UniqueGiftDialoguesExpanded, unfeldspar.More_Specific_Gift_Reaction-Dialogue_Expansion-eng, Kaori_Haruta.More_Specific_Gift_Reaction-Dialoge_Expansion-zh": "true"
      }
    },
    //Unique Gift Dialogue Compat
    {
      "Name": "ModdedDialogCompat.UGDE",
      "Value": "true" //generally my content should be applied
    },
    {
      "Name": "ModdedDialogCompat.UGDE",
      "Value": "{{Random: true, false}}", //randomize applying my content when these mods are present
      "When": {
        "HasMod |contains= PrincessFelicie.UniqueGiftDialoguesExpanded": "true" //use this token option when only UGDE needs accounting for
      }
    },
    //More Specific Gift Reactions Compat
    {
      "Name": "ModdedDialogCompat.MSGR",
      "Value": "true" //generally my content should be applied
    },
    {
      "Name": "ModdedDialogCompat.MSGR",
      "Value": "{{Random: true, false}}", //randomize applying my content when these mods are present
      "When": {
        "HasMod |contains= unfeldspar.More_Specific_Gift_Reaction-Dialogue_Expansion-eng, Kaori_Haruta.More_Specific_Gift_Reaction-Dialoge_Expansion-zh": "true" //use this token option when only MSGR needs accounting for
      }
    },
    //Haywrite's Dialogue Compat
    {
      "Name": "ModdedDialogCompat.Hay",
      "Value": "true" //generally my content should be applied
    },
    {
      "Name": "ModdedDialogCompat.Hay",
      "Value": "{{Random: true, false}}", //randomize applying my content when these mods are present
      "When": {
        "HasMod |contains= silver.haywrite.dialogue": "true" //use this token option only for Haywrite's Dialogue!
      }
    },

    //Festival Compat
    {
      "Name": "SyncedRandom",
      "Value": "{{Random: True, False |key=CFE_SyncedRandom}}" //used for syncing edits to CFE's festival dialogue. Other mods can also sync any compatibility edits in their own code by copying this token and key and including it in their content.json
