{
"Format": "2.0.0",
"ConfigSchema": {
		
		"Trees": {
			"AllowValues": "true, false",
			"Default": "true"
		},
		"MushTrees": {
			"AllowValues": "true, false",
			"Default": "true"
		},
		"TileTrees": {
			"AllowValues": "true,false",
			"Default": "true"
		},
		"TileBushes": {
			"AllowValues": "true,false",
			"Default": "true"
		},
		"FruitTrees": {
			"AllowValues": "true,false",
			"Default": "true"
		},
		"Bushes": {
			"AllowValues": "true,false",
			"Default": "true"
		},
		"ManualRecolor": {
			"AllowValues": "VPR, Vanilla, Starblue, Earthy, Naturally, Wittily",
			"AllowBlank":true,
			"Default": ""
		},
		"WinterLeaves": {
			"AllowValues": "true, false",
			"Default": "true"
		},
		"AltBush": {
			"AllowValues": "true, false",
			"Default": "true"
		},
		"BushFlowers": {
			"AllowValues": "true, false",
			"Default": "true"
		},
		"VPRFallColor": {
			"AllowValues": "green, orange",
			"Default": "orange"
		},
		"StarbluePinkTrees": {
			"AllowValues": "true, false",
			"Default": "false"
		},
		"StarblueWhiteTrees": {
			"AllowValues": "true, false",
			"Default": "false"
		}
	},

	"DynamicTokens": [
			{
			"Name": "Recolor",
			"Value": "Vanilla"
			},
			{
			"Name": "Recolor",
			"Value": "Earthy",
			"When": { "HasMod": "DaisyNiko.EarthyRecolour" }
			},
			{
			"Name": "Recolor",
			"Value": "Starblue",
			"When": { "HasMod": "Lita.StarblueValley" }
			},
			{
			"Name": "Recolor",
			"Value": "VPR",
			"When": { "HasMod": "grapeponta.VibrantPastoralRecolor, poohnhi.VPRTempFix" }
			}, 
			{
			"Name": "Recolor",
			"Value": "Naturally",
			"When": { "HasMod": "Narianka.NaturallyValley" }
			},
			{
			"Name": "Recolor",
			"Value": "Wittily",
			"When": { "HasMod": "Acerbicon.Recolor" }
			}, 
			{
			"Name": "Recolor",
			"Value": "{{ManualRecolor}}",
			"When": { "ManualRecolor |contains=Vanilla, VPR, Starblue, Earthy, Naturally, Wittily": true }
			},
			{
			"Name": "Recolor",
			"Value": "{{Recolor}}/{{VPRFallColor}}",
			"When": { "Recolor |contains=VibrantPastoral": true,
					"Season": "fall" }
			}
		],

