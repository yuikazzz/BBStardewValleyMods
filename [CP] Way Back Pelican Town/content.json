/* 					
					Hi there!

					Editing anything here changes how the mod works.
					If you want to disable something/change option -> edit config.json instead!
					It will be generated after SMAPI loads the mod first time into the game c:
					
*/


	"Format": "2.0.0",
	
	
	
	"ConfigSchema": {
		"FallColor":{"AllowValues": "1, 2",
					"Default": "1",
					"Description": "To match recolor mods that have that option -> green(1) orange(2)" },
		"RecolorOverwrite":{"AllowValues": "Vanilla, DNEarthyRecolour, Eemie, FoliageRedone, Starblue, VibrantPastoral, WittilyNamed",
					"AllowBlank": true,
					"Default": "",
					"Description": "If you want to change automaticcaly matched recolor palette" },
					
		"PlaygroudOption":{"AllowValues": "playground, clothesline with well",
					"Default": "playground",
					"Description": "Retexture of the playground area next to CC" },
		"FencesOption":{"AllowValues": "stone, wood",
					"Default": "stone",
					"Description": "Retexture of the static map fences" },
		"LuauMusicPlayer":{"AllowValues": "1, 2",
					"Default": "1",
					"Description": "1- record player, 2- junimos drummers" },
		"WeddingArchOption":{"AllowValues": "1, 2, 3, 4, off",
					"Default": "1",
					"Description": "1- cottagecore, 2- sunflowers, 3- curtain, 4- boho, off- disabled" },
		"GrangeDisplayDecor":{"AllowValues": "true ,false",
					"Default": "true",
					"Description": "Additional decor on your fair Grange Display" },
		"JojaChanged":{"AllowValues": "true, false",
					"Default": "true",
					"Description": "Disable if you don't want joja to look like the castle" },
		"BusChanged":{"AllowValues": "true, false",
					"Default": "true",
					"Description": "Disable if you don't want the Bus retextured" },
		"TrashCanHat":{"AllowValues": "true, false",
					"Default": "true" },
	},
	
	
	
	"DynamicTokens": [
	//checking what recolor is used
		{
		"Name": "recolor",
		"Value": "Vanilla",
		},
		{
		"Name": "recolor",
		"Value": "DNEarthyRecolour",
			"When": { "HasMod": "DaisyNiko.EarthyRecolour" },
		},
		{
		"Name": "recolor",
		"Value": "Eemie",
			"When": { "HasMod": "minervamaga.CP.eemieMapRecolour" },
		},
		{
		"Name": "recolor",
		"Value": "FoliageRedone",
			"When": { "HasMod": "stardewfoliageredone" },
		},
		{
		"Name": "recolor",
		"Value": "Starblue",
			"When": { "HasMod": "Lita.StarblueValley" },
		},
		{
		"Name": "recolor",
		"Value": "VibrantPastoral",
			"When": { "HasMod": "grapeponta.VibrantPastoralRecolor" },
		},
		{
		"Name": "recolor",
		"Value": "WittilyNamed",
			"When": { "HasMod": "Acerbicon.Recolor" },
		},

	//recolor overwrite
		{
		"Name": "recolor",
		"Value": "{{RecolorOverwrite}}",
			"When": { "RecolorOverwrite |contains=Vanilla, DNEarthyRecolour, Eemie, FoliageRedone, Starblue, VibrantPastoral, WittilyNamed": true },
		},
	//fall color overwrite
		{
		"Name": "recolor",
		"Value": "{{recolor}}/{{FallColor}}",
			"When": { "recolor |contains= Eemie, VibrantPastoral": true,
					"Season": "fall" },
		},
