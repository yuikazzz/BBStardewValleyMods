{"Format": "2.4.3", 

	"ConfigSchema": {
	"ModEnabled" : 
	{"AllowValues": "all year, by season", "Default" : "all year"}, 
	
	"Dialogue": 
	{"AllowValues": "true, false", "Default" : "true"}
	},
	
		"Changes": 
		[
		{
			
            "Action": "Include",
			"FromFile": "assets/all year.json",
			"When":
			{"ModEnabled": "all year"}
			},
			
			{
			"Action": "Include",
			"FromFile": "assets/by season.json",
		
			"When":
			{ "ModEnabled": "by season"}
			},
			
			{
			"Action": "Include",
			"FromFile": "Dialogue/Dialogue Gus.json",
			"When": {"Dialogue": "true"},
			},
			
			
			
		]
}