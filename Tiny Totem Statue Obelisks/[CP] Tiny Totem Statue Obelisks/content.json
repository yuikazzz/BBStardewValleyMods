{
  "Format": "2.0.6",
  "ConfigSchema": {
    "EarthObeliskEnabled": {
      "AllowValues": "true, false",
      "Default": "true",
      "Section": "Obelisks"
    },
    "DesertObeliskEnabled": {
      "AllowValues": "true, false",
      "Default": "true",
      "Section": "Obelisks"
    },
    "WaterObeliskEnabled": {
      "AllowValues": "true, false",
      "Default": "true",
      "Section": "Obelisks"
    },
    "IslandObeliskEnabled": {
      "AllowValues": "true, false",
      "Default": "true",
      "Section": "Obelisks"
    },
    "FarmObeliskEnabled": {
      "AllowValues": "true, false",
      "Default": "true",
      "Section": "Obelisks"
    },
    "EastScarpEnabled": {
      "AllowValues": "true, false",
      "Default": "true",
      "Section": "Compatibility"
    },
    "RidgesideVillageEnabled": {
      "AllowValues": "true, false",
      "Default": "true",
      "Section": "Compatibility"
    },
    "RemoteObeliskEnabled": {
      "AllowValues": "true, false",
      "Default": "true",
      "Section": "Remote"
    },
    "RemoteWarpLocation": {
      "AllowValues": "default, custom",
      "Default": "default",
      "Section": "Remote"
    },
    "CustomFarmX": {
      "Default": 48,
      "Section": "Remote",
      "Description": "Can also be used as an override for the Remote Obelisk teleport location."
    },
    "CustomFarmY": {
      "Default": 7,
      "Section": "Remote",
      "Description": "Can also be used as an override for the Remote Obelisk teleport location."
    },
    "FlowersEnabled": {
      "AllowValues": "true, false",
      "Default": "true",
      "Section": "Decoration"
    },
    "MossEnabled": {
      "AllowValues": "true, false",
      "Default": "true",
      "Section": "Decoration"
    },
    "MiniObeliskEnabled": {
      "AllowValues": "true, false",
      "Default": "true",
      "Section": "Mini-Obelisks"
    },
    "MiniObeliskStyle": {
      "AllowValues": "signpost, statue",
      "Default": "signpost",
      "Section": "Mini-Obelisks"
    }
  },
  "DynamicTokens": [
    {
      "Name": "RemoteX",
      "Value": "{{CustomFarmX}}",
      "When": {
        "RemoteWarpLocation": "custom"
      }
    },
