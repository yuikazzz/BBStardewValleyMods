{
    "Format": "2.0.0",

"ConfigSchema": {
        "ManualRecolor": {
            "AllowValues": "VPR, Vanilla, Earthy, Starblue, Wittily",
            "AllowBlank":true,
            "Default": ""
        }
    },
    "DynamicTokens": [
        {
        "Name": "Recolor",
        "Value": "Vanilla"
        },
        {
		"Name": "Recolor",
		"Value": "Earthy",
		"When": { "HasMod": "DaisyNiko.EarthyRecolour" }
		},
        {
        "Name": "Recolor",
        "Value": "Starblue",
        "When": { "HasMod": "Lita.StarblueValley" }
        },
        {
        "Name": "Recolor",
        "Value": "VPR",
        "When": { "HasMod": "grapeponta.VibrantPastoralRecolor, poohnhi.VPRTempFix" }
        }, 
        {
        "Name": "Recolor",
        "Value": "Wittily",
        "When": { "HasMod": "Acerbicon.Recolor" }
        }, 
        {
        "Name": "Recolor",
        "Value": "{{ManualRecolor}}",
        "When": { "ManualRecolor |contains=Vanilla, VPR, Starblue, Wittily": true }
        }
    ],

